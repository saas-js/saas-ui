{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "*/dist/**"],
      "env": [
        "SUPABASE_URL",
        "SUPABASE_ANON_KEY",
        "AUTH_ORIGIN",
        "REGISTRY_URL",
        "SCHEMA_URL"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:staged": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
