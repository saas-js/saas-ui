{
  "name": "@saas-ui/react",
  "version": "3.0.0-next.45",
  "description": "Saas UI - The React component library for startups.",
  "type": "module",
  "exports": {
    ".": {
      "sui": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preset": {
      "sui": "./src/preset.ts",
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.js"
    },
    "./colors": {
      "sui": "./src/colors.ts",
      "types": "./dist/colors.d.ts",
      "import": "./dist/colors.js"
    },
    "./*": {
      "sui": "./src/components/*/index.ts",
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf --no-glob ./dist",
    "build:tsup": "NODE_OPTIONS='--max-old-space-size=16384' tsup --config tsup.config.ts ",
    "build:components": "tsup --config tsup.components.config.ts --dts-only",
    "build:types": "tsc --emitDeclarationOnly --outDir dist --project tsconfig.json",
    "prebuild": "pnpm clean",
    "build": "tsc --outDir dist --project tsconfig.json",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --config ../../eslint.config.js",
    "lint:staged": "lint-staged --allow-empty --config ../../lint-staged.config.js",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Eelco Wiersma <eelco@saas-ui.dev>",
  "license": "MIT",
  "homepage": "https://saas-ui.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saas-js/saas-ui.git",
    "directory": "packages/saas-ui-react"
  },
  "keywords": [
    "react",
    "ui",
    "chakra-ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "emotion",
    "library"
  ],
  "storybook": {
    "title": "Saas UI",
    "url": "https://storybook.saas-ui.dev"
  },
  "dependencies": {
    "@ark-ui/react": "^5.25.1",
    "@saas-ui/chakra-preset": "workspace:*",
    "@saas-ui/core": "workspace:*",
    "@saas-ui/hooks": "workspace:*",
    "next-themes": "^0.4.6"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^3.27.0",
    "@emotion/react": "^11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  }
}
