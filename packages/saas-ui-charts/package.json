{
  "name": "@saas-ui/charts",
  "version": "0.15.0",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --config tsup.config.ts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chakra-ui/styled-system": "^2.12.1",
    "recharts": "^2.13.0"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=2.9.0 <3",
    "@emotion/react": ">=11",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.2",
    "date-fns": "^3.6.0",
    "prop-types": "^15.8.1",
    "tsup": "^8.3.0"
  }
}
