  "homepage": "https://saas-ui.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saas-js/saas-ui.git",
    "directory": "packages/saas-ui-nprogress"
  },
  "keywords": [
    "react",
    "ui",
    "chakra-ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "emotion",
    "library",
    "design-system"
  ],
  "storybook": {
    "title": "Saas UI",
    "url": "https://storybook.saas-ui.dev"
  },
  "dependencies": {
    "@tanem/react-nprogress": "^5.0.53"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^3.2.1",
    "@saas-ui/core": "workspace:*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
