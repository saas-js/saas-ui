    "@types/node": "^20.10.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "fast-glob": "^3.3.2",
    "prettier": "^3.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
